{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/*.bat",
      "**/*.sh",
      ".git/**",
      ".github/**"
    ],
    "rewrites": [
      {
        "source": "!{configurar-usuarios.html}",
        "destination": "/index.html"
      }
    ]
  }
}
